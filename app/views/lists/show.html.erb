<h2 class="inline"><%= @list.name %></h2> - <%= @list.created_on %>
<% @tasks.each do |task| %>
  <div>
    <%= link_to task.content, task_path(task) %>
  </div>
<% end %>
<br>
<%= link_to "Rename", edit_list_path(@list) %>
<%= link_to "Delete", list_path(@list), method: :delete %>
<%= link_to "Add Task", new_list_task_path(@list) %>

